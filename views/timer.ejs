    <script>
      // SUBS CHECKER :
      function subs_checker() {
        let userid = document.querySelector("#header").dataset.userid;
        return fetch(`/check/${userid}`)
          .then((response) => response.json())
          .then((responseData) => {
            return responseData;
          })
          .catch((error) => console.log(error));
      }

      function createCookie(name, value, maxAge_s) {
        var expires;

        if (maxAge_s) {
          var date = new Date();
          date.setTime(date.getTime() + maxAge_s * 1000);
          expires = "; expires=" + date.toGMTString();
        } else {
          expires = "";
        }

        document.cookie = name + "=" + value + expires + "; path=/";
      }

      // Read cookie
      function readCookie(name) {
        var nameEQ = name + "=";
        var ca = document.cookie.split(";");

        for (var i = 0; i < ca.length; i++) {
          var c = ca[i];
          while (c.charAt(0) === " ") {
            c = c.substring(1, c.length);
          }
          if (c.indexOf(nameEQ) === 0) {
            return c.substring(nameEQ.length, c.length);
          }
        }

        return null;
      }

      // Erase cookie
      function eraseCookie(name) {
        createCookie(name, "", -1);
      }

      let curr_subs;
      let timer = document.querySelector("#timer");
      let refresh_btn = document.querySelector(".refresh_msg");
      let yt_v = document.querySelector("#yt_v");
      let waitBtn = document.querySelector(".wait-btn");
      let getLink = document.querySelector(".getLink");
      let time = 5;
      // subs
      let He_subs = false;
      let He_watched = false;

      //  Watch Checker :
      let watch_time = 10;
      // value * 5s
      let lil_interval_time = 10;
      // 15s
      let sec = lil_interval_time * 5;
      //
      //
      //
      //
      //
      //
      //
      // GET YT INFO
      function get_yt_Links() {
        let userid = document.querySelector("#header").dataset.userid;
        let user_ch = document.querySelector("#header").dataset.ch;
        let yt_btn_sub = document.querySelector("#yt_btn_sub");

        fetch(`/check/${userid}`)
          .then((res) => res.json())
          .then((data) => {
            if (data) {
              const yt_vid =
                "https://www.youtube.com/watch?v=" +
                data.v_data[Math.floor(Math.random() * data.v_data.length)]
                  .video_id;

              yt_v.href = yt_vid;
              yt_btn_sub.href = user_ch;
            }
          })
          .catch((error) => console.log(error));
      }
      get_yt_Links();
      //
      //
      //
      //
      //
      //
      //
      yt_v.addEventListener("click", () => {
        var timerId = setInterval(countdown, 1000);

        function countdown() {
          // console.log("Time left : " + watch_time);
          if (watch_time <= -1) {
            He_watched = true;
            clearTimeout(timerId);
          } else {
            watch_time--;
          }
        }
      });

      //  Compare subs :
      function compare_subs(prev_subs) {
        subs_checker().then((response) => {
          if (response.subs > prev_subs) {
            He_subs = true;
            // Create token :
            createCookie("sub_token", 1, 10);
          } else {
            eraseCookie("sub_token");
            He_subs = false;
          }
        });
      }

      let secon;
      function left_time_counter() {
        secon = setInterval(() => {
          waitBtn.innerText = "هيا أسرع (" + sec + ")";
          if (sec == 0) {
            clearInterval(secon);
            waitBtn.innerText = "انتهى الوقت ستتم اعادتك";
            // refresh the page :

            refresh_btn.classList.remove("d-none");
            refresh_btn.classList.remove("disabled");
            refreshPage();
          }
          sec--;
        }, 1000);
      }

      // check subs count :
      subs_checker().then((response) => {
        curr_subs = response.subs;

        // 1s interval
        let big_interval = setInterval(function () {
          time--;
          if (time >= 0) {
            timer.innerHTML = time;
          }

          let exe_time = 0;

          if (time == 0) {
            // Hurry function
            left_time_counter();
          }
          if (time <= 0) {
            // clear the big interval :
            clearInterval(big_interval);

            // 5s interval
            let lil_interval = setInterval(function () {
              // Subs conditions
              if ((time <= 0 && He_subs) || (time <= 0 && He_watched)) {
                clearInterval(secon);
                waitBtn.innerText = "الرابط جاهز";
                waitBtn.classList.remove("disabled");

                waitBtn.addEventListener("click", () => {
                  getLink.classList.remove("d-none");
                  getLink.classList.remove("disabled");
                });
                const redirectMe = () => {
                  window.location.href = "/test2";
                };
                getLink.addEventListener("click", redirectMe);
              }

              // Exe Timer :
              exe_time++;
              // compare subs :
              compare_subs(curr_subs);
              // clear interval :
              if (exe_time == lil_interval_time) {
                clearInterval(lil_interval);
              }
            }, 5000);
          }
        }, 1000);
      });
